<h1>New tst_conn</h1>

<% form_for(@tst_conn) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :connection_id %><br />
    <%= f.text_field :connection_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', tst_conns_path %>